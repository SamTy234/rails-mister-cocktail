<h1>Create a dose</h1>
<div class="create-form">
  <%= simple_form_for([@cocktail, @dose]) do |f| %>
    <div class="form-inputs">
    <%= f.input :description %>
    <%= f.association :ingredient%>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
<%= link_to "Back", cocktail_path(@cocktail), class: "btn btn-primary"  %>


</div>


